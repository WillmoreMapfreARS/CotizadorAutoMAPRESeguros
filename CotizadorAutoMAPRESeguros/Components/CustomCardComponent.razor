@using MudBlazor


<MudCard Elevation="5">
    <MudCardContent>
        <MudImage Src="@imgSrc" Style="align-content:center; margin-left:5px; text-align:center; width: 110px !important; height:110px !important"></MudImage>
        <MudText Typo="Typo.h6" Align="Align.Center" Style="color:#6B6B6B">@descripcion</MudText>
    </MudCardContent>
</MudCard>

@code {
    [Parameter]
    public string imgSrc { get; set; } = string.Empty;
    [Parameter]
    public string descripcion { get; set; } = string.Empty;
}
